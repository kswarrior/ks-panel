{
  "name": "ks-panel",
  "version": "2.0.0",
  "description": "KS Panel is an open-source web panel for managing game servers and applications built with Node.js, Express, and Docker.",
  "main": "index.js",
  "bin": {
    "ks-panel": "./exec/bin/index.js"
  },
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "dev:full": "concurrently \"npm run dev\" \"npm run watch\"",
    "watch": "browser-sync start --config bs-config.js",
    "seed": "node exec/seed.js",
    "create:user": "node exec/createUser.js",
    "build:css": "npx @tailwindcss/cli -i ./public/tw.conf -o ./public/assets/tailwind.css --watch"
  },
  "author": "ks-panel-team",
  "license": "MIT",
  "dependencies": {
    "@keyv/sqlite": "^3.6.7",
    "@keyvhq/core": "^2.1.7",
    "@keyvhq/mysql": "^2.1.7",
    "@tailwindcss/cli": "^4.1.17",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "better-sqlite3": "^11.1.2",
    "better-sqlite3-session-store": "^0.1.0",
    "body-parser": "^1.20.3",
    "cat-loggr": "^1.2.2",
    "chalk": "^4.1.2",
    "commander": "^12.1.0",
    "concurrently": "^8.2.2",
    "cookie-parser": "^1.4.7",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "express-basic-auth": "^1.2.1",
    "express-rate-limit": "^7.4.1",
    "express-session": "^1.18.2",
    "express-ws": "^5.0.2",
    "form-data": "^4.0.4",
    "keyv": "^4.5.4",
    "multer": "^2.0.2",
    "node-schedule": "^2.1.1",
    "nodemailer": "^6.9.15",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "qrcode": "^1.5.4",
    "readline": "^1.3.0",
    "speakeasy": "^2.0.0",
    "tailwindcss": "^4.1.17",
    "uuid": "^10.0.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "browser-sync": "^2.26.2",
    "nodemon": "^3.1.10"
  },
  "engines": {
    "node": ">=18"
  }
}
